#include <stdio.h>
int main()
{
    int m;
    printf("Enter the mark: ");
    scanf("%d",&m);

    if(m>=90)
        printf("S Grade");
    else if(m<90 && m>=80)
        printf("A Grade");
    else if(m<80 && m>=70)
        printf("B Grade");
    else if(m<70 && m>=50)
        printf("C Grade");
    else
        printf("Fail");
